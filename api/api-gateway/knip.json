{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "ignore": ["src/**/*.enum.ts"],
    "entry": ["src/main.ts"],
    "project": ["src/**/*.ts"],
    "rules": {
        "files": "error",
        "classMembers": "error",
        "types": "error",
        "dependencies": "warn",
        "devDependencies": "warn"
    }
}
